{
  "name": "wpl/events-tracker-for-elementor",
  "description": "Track Click or Submit events and conversions for any Elementor widget with Google Analytics, Facebook, Yandex Metrika, Vkontakte.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "WPL\\Events_Tracker_For_Elementor\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Mikhail Kobzarev",
      "email": "mikhail@kobzarev.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.10",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "wp-coding-standards/wpcs": "^3.1",
    "vimeo/psalm": "^5.26",
    "humanmade/psalm-plugin-wordpress": "^3.1"
  },
  "scripts": {
    "phpcs": "phpcs --standard=./phpcs.xml",
    "phpcbf": "phpcbf --standard=./phpcs.xml",
    "psalm": "psalm --config=psalm.xml"
  }
}
